{
    "name": "youtube-clone/admin-service",
    "description": "Admin Dashboard and CMS Service for YouTube Clone - Symfony Application",
    "type": "project",
    "license": "ISC",
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*",
        "symfony/console": "8.0.*",
        "symfony/dotenv": "8.0.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "8.0.*",
        "symfony/runtime": "8.0.*",
        "symfony/yaml": "8.0.*",
        "symfony/http-client": "8.0.*",
        "symfony/orm-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/twig-bundle": "8.0.*",
        "symfony/form": "8.0.*",
        "symfony/validator": "8.0.*",
        "symfony/mailer": "8.0.*",
        "doctrine/doctrine-bundle": "^2.13",
        "doctrine/orm": "^3.3"
    },
    "require-dev": {
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "8.0.*"
        }
    }
}
